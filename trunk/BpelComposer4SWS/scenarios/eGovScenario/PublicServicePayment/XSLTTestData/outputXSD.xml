<?xml version="1.0" encoding="UTF-8"?>
<makePaymentReturn xsi:type="ns2:receipt" xmlns:ns2="http://localhost:8080/PublicServices/PublicServicePayment/XPublicServicePayment" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<confirmedPayment xsi:type="ns2:payment">
		<sourceAccount xsi:type="ns2:account">
			<accountOwner xsi:type="xsd:string">srcOwner</accountOwner>
			<accountNumber xsi:type="xsd:string">srcNumber</accountNumber>
			<bankID xsi:type="xsd:string">srcBankID</bankID>
		</sourceAccount>
		<destAccount xsi:type="ns2:account">
			<accountOwner xsi:type="xsd:string">destOwner</accountOwner>
			<accountNumber xsi:type="xsd:string">destNumber</accountNumber>
			<bankID xsi:type="xsd:string">destbankID</bankID>
		</destAccount>
		<amount xsi:type="xsd:string">10.00</amount>
		<reasonForPayment xsi:type="xsd:string">aReason</reasonForPayment>
	</confirmedPayment>
	<confirmation xsi:type="xsd:string">ok</confirmation>
</makePaymentReturn>
